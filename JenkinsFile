
pipeline {
    agent any
    stages {
	stage('Clone Repo') {
            steps {
                sh 'git fetch https://github.com/ALowtonQA/Spring_Boot_Project.git'
            }
	}

        stage('Run Tests') {
            steps {
                sh 'cd Spring_Boot_Project && java -jar MusicHWA-0.0.1-SNAPSHOT.jar'

            }
	}
        stage('Test Report') {
            steps {
                sh 'ls'
            }
        }
	stage('Build Jar') {
            steps {
                sh 'ls'
            }
        }
        stage('Deploy') {
            steps {
      		sh 'ls'
            }
        }
    }
}
