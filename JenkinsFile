
pipeline {
    agent any
    stages {
	stage('Clone Repo') {
            steps {
                sh 'git fetch https://github.com/ALowtonQA/Spring_Boot_Project.git'
		sh 'cd Spring_Boot_Project'
            }
        }
        stage('Run Tests') {
            steps {
                sh 'cd Spring_Boot_Project && ls'
		junit 'Spring_Boot_Project/src/test/java/com/qa/musichwa/controller/MusicControllerUnitTest.java'
            }
        }
        stage('Test Report') {
            steps {
                sh 'ls'
            }
        }
	stage('Build Jar') {
            steps {
                sh 'ls'
            }
        }
        stage('Deploy') {
            steps {
      		sh 'ls'
            }
        }
    }
}
